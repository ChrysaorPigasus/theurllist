{
  "name": "theurllist",
  "type": "commonjs",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "db:init": "psql -d theurllist -f src/utils/init.sql",
    "lint": "eslint src/**/*.{js,jsx,astro}",
    "format": "prettier --write src/**/*.{js,jsx,astro,css}",
    "test": "run-s test:unit test:api test:e2e test:bdd",
    "test:fast": "run-p test:unit test:api",
    "test:local": "cross-env TEST_ENV=local run-p test:unit test:api test:e2e test:bdd",
    "test:dev": "cross-env TEST_ENV=dev run-s test:unit",
    "test:tst": "cross-env TEST_ENV=tst run-p test:unit test:api",
    "test:acc": "cross-env TEST_ENV=acc run-p test:e2e test:bdd",
    "test:with-mocks": "cross-env FORCE_MOCKS=true npm test",
    "test:with-integrations": "cross-env FORCE_INTEGRATIONS=true npm test",
    "test:unit": "cross-env TEST_TYPE=unit vitest run",
    "test:unit:watch": "vitest --watch",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest --coverage",
    "test:unit:coverage:report": "vitest --coverage --reporter html",
    "test:unit:coverage:report:open": "vitest --coverage --reporter html && open coverage/index.html",
    "test:api": "cross-env TEST_TYPE=api vitest run tests/api",
    "test:api:playwright": "cross-env NODE_ENV=test npx playwright test --config=tests/api/playwright.config.cjs",
    "test:api:dev": "cross-env TEST_ENV=dev TEST_TYPE=api vitest run tests/api",
    "test:api:tst": "cross-env TEST_ENV=tst TEST_TYPE=api vitest run tests/api",
    "test:api:acc": "cross-env TEST_ENV=acc TEST_TYPE=api vitest run tests/api",
    "test:api:playwright:dev": "cross-env TEST_ENV=dev TEST_TYPE=api npx playwright test --config=tests/api/playwright.config.cjs",
    "test:api:playwright:tst": "cross-env TEST_ENV=tst TEST_TYPE=api npx playwright test --config=tests/api/playwright.config.cjs",
    "test:api:playwright:acc": "cross-env TEST_ENV=acc TEST_TYPE=api npx playwright test --config=tests/api/playwright.config.cjs",
    "test:e2e": "cross-env TEST_TYPE=e2e playwright test",
    "test:e2e:dev": "cross-env TEST_ENV=dev TEST_TYPE=e2e playwright test",
    "test:e2e:tst": "cross-env TEST_ENV=tst TEST_TYPE=e2e playwright test",
    "test:e2e:acc": "cross-env TEST_ENV=acc TEST_TYPE=e2e playwright test",
    "test:bdd": "cross-env TEST_TYPE=bdd cucumber-js",
    "test:bdd:dev": "cross-env TEST_ENV=dev TEST_TYPE=bdd cucumber-js",
    "test:bdd:tst": "cross-env TEST_ENV=tst TEST_TYPE=bdd cucumber-js",
    "test:bdd:acc": "cross-env TEST_ENV=acc TEST_TYPE=bdd cucumber-js",
    "test:bdd:playwright": "cross-env TEST_TYPE=bdd playwright test --config=tests/bdd/playwright.config.cjs",
    "test:bdd:playwright:dev": "cross-env TEST_ENV=dev TEST_TYPE=bdd playwright test --config=tests/bdd/playwright.config.cjs",
    "prepare:bdd": "node scripts/convert-steps-to-commonjs.js",
    "test:bdd:fixed": "npm run prepare:bdd && cross-env TEST_TYPE=bdd cucumber-js --require=./tests/bdd/cucumber-setup.cjs",
    "test:orchestrate": "ts-node scripts/test-orchestration.ts",
    "test:orchestrate:parallel": "ts-node scripts/test-orchestration.ts --parallel",
    "test:orchestrate:turbo": "ts-node scripts/test-orchestration.ts --turbo",
    "test:orchestrate:unit": "ts-node scripts/test-orchestration.ts unit",
    "test:orchestrate:api": "ts-node scripts/test-orchestration.ts api",
    "test:orchestrate:e2e": "ts-node scripts/test-orchestration.ts e2e",
    "test:orchestrate:bdd": "ts-node scripts/test-orchestration.ts bdd",
    "test:config": "ts-node scripts/test-config-report.ts",
    "test:turbo": "turbo test",
    "test:turbo:unit": "turbo test:unit",
    "test:turbo:api": "turbo test:api",
    "test:turbo:e2e": "turbo test:e2e",
    "test:turbo:bdd": "turbo test:bdd",
    "test:turbo:local": "cross-env TEST_ENV=local turbo test:unit test:api test:e2e test:bdd",
    "test:turbo:dev": "cross-env TEST_ENV=dev turbo test:unit",
    "test:turbo:tst": "cross-env TEST_ENV=tst turbo test:unit test:api",
    "test:turbo:acc": "cross-env TEST_ENV=acc turbo test:e2e test:bdd",
    "test:fix-paths": "node scripts/fix-test-paths.js"
  },
  "dependencies": {
    "@astrojs/react": "^4.0.0",
    "@astrojs/tailwind": "^5.1.5",
    "@nanostores/react": "^0.7.1",
    "astro": "^5.6.1",
    "nanostores": "^0.9.3",
    "pg": "^8.14.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "postgres": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/register": "^7.25.9",
    "@cucumber/cucumber": "^11.2.0",
    "@playwright/test": "^1.52.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cucumber": "^7.0.3",
    "@types/node": "^22.14.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "cross-env": "^7.0.3",
    "cucumber": "^6.0.7",
    "dotenv": "^16.5.0",
    "dotenv-flow": "^4.1.0",
    "eslint": "^8.45.0",
    "eslint-plugin-astro": "^0.30.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.0",
    "glob": "^11.0.1",
    "happy-dom": "^17.4.4",
    "jsdom": "^26.1.0",
    "module-alias": "^2.2.3",
    "msw": "^2.7.4",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.51.1",
    "prettier": "^3.0.0",
    "prettier-plugin-astro": "^0.12.0",
    "ts-node": "^10.9.2",
    "vitest": "^3.1.1"
  }
}
